{
    "name": "huubl/silverstripe-elemental-photo-collage",
    "description": "A photo collage to add to your page.",
    "authors": [
        {
            "name": "Huubl",
            "email": "contact@studiolunter.nl"
        }
    ],
    "keywords": [
        "silverstripe", "elemental", "content", "blocks", "image", "collage"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "dnadesign/silverstripe-elemental": "^2@dev || ^3@dev",
        "silverstripe/recipe-cms": "^1@dev || ^4@dev",
        "silverstripe/vendor-plugin": "^1@dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "Huubl\\Elements\\Collage\\": "src/",
            "Huubl\\Elements\\Collage\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client"
        ]
    }
}
